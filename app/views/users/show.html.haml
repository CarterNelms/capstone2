- if @user == current_user
  =link_to "Edit Profile", edit_user_registration_path

%ul
  - @user.library.items.each do |i|
    %li= link_to i.name, item_path(i)
- if @user == current_user
  =link_to "Add New Item", new_item_path, id: "new_library_item"

%ul
  - @user.wanted_list.items.each do |i|
    %li= link_to i.name, item_path(i)
- if @user == current_user
  =link_to "Add New Item", items_new_wanted_path, id: "new_wanted_list_item"
